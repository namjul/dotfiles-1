{
  "title": "Option special chars (Dvorak IULM layout)",
  "rules": [
    {
      "description": "Change Option E to {",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "hyphen",
              "modifiers": "left_shift"
            }
          ]
        }
      ]
    },
    {
      "description": "Change Option R to }",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "equal_sign",
              "modifiers": "left_shift"
            }
          ]
        }
      ]
    },
    {
      "description": "Change Option D to [",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "hyphen"
            }
          ]
        }
      ]
    },
    {
      "description": "Change Option F to ]",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ]
        }
      ]
    },
    {
      "description": "Change Option C to (",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": "left_shift"
            }
          ]
        }
      ]
    },
    {
      "description": "Change Option V to )",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "0",
              "modifiers": "left_shift"
            }
          ]
        }
      ]
    }
  ]
}
